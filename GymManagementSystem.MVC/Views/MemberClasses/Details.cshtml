@model GymMs.DAL.GymMs.DAL.Entities.MemberClass

@{
    ViewData["Title"] = "Details";
}

<h2>Member-Class Details</h2>

<div>
    <h4>MemberClass</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Member</dt>
        <dd class="col-sm-10">@Model.Member?.FullName</dd>

        <dt class="col-sm-2">Class</dt>
        <dd class="col-sm-10">@Model.GymClass?.Title</dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-memberId="@Model.MemberId" asp-route-gymClassId="@Model.GymClassId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
